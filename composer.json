{
    "name": "filippo-toso/dialogflow-backup",
    "description": "Dialogflow agent backup and restore component",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Toso",
            "email": "filippo@toso.dev"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "illuminate/support": ">=5.7",
        "google/cloud-dialogflow": "^0.13.0"
    },
    "autoload": {
        "psr-4": {
            "FilippoToso\\DialogflowBackup\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilippoToso\\DialogflowBackup\\ServiceProvider"
            ],
            "aliases": {
                "DialogflowBackup": "FilippoToso\\DialogflowBackup\\Facade"
            }
        }
    }
}
